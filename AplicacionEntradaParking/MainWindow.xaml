<Window x:Class="AplicacionEntradaParking.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AplicacionEntradaParking"
        mc:Ignorable="d"
        Title="Entrada al Parking" Height="450" Width="800" WindowStyle="None" ResizeMode="NoResize" WindowState="Maximized"
        Closing="Window_Closing" KeyDown="Window_KeyDown" Icon="images/icono-ticket.ico">
    <Grid>
        <Button Command="{Binding Path=EntradaVehiculoCommand}" Background="Aqua">
        <Image Source="images/ticket.png"></Image>
        </Button>
    </Grid>
</Window>
